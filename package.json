{
  "name": "bloomcrawler-riis",
  "version": "1.0.0",
  "description": "BLOOMCRAWLER RIIS - Recursive Image Intervention System",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "tsx server.ts",
    "start:dev": "tsx watch server.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build:exe": "pkg standalone-server.cjs --compress GZip",
    "build:exe:win": "pkg standalone-server.cjs --compress GZip --targets node18-win-x64 -o dist/bloomcrawler.exe",
    "build:exe:complete": "pkg complete-standalone-server.cjs --compress GZip --targets node18-win-x64 -o dist/bloomcrawler-complete.exe",
    "build:exe:all-modules": "pkg complete-standalone-server.cjs --compress GZip --targets node18-win-x64,node18-win-x86,node18-linux-x64,node18-macos-x64",
    "build:exe:all": "pkg standalone-server.cjs --compress GZip --targets node18-win-x64,node18-win-x86,node18-linux-x64,node18-macos-x64",
    "postbuild:exe": "echo Built executable in dist/ directory",
    "start:clean": "node server-clean.cjs",
    "build:clean-exe": "pkg server-clean.cjs --compress GZip --targets node18-win-x64 -o dist/bloomcrawler-clean.exe"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.0.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.7.1",
    "better-sqlite3": "^12.4.1",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "drizzle-orm": "^0.44.7",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.0",
    "helmet": "^8.1.0",
    "lucide-react": "^0.294.0",
    "mysql2": "^3.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.10.0",
    "socket.io": "^4.8.1",
    "socks": "^2.8.7",
    "superjson": "^2.2.5",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.13.1",
    "drizzle-kit": "^0.31.6",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "pkg": "^5.8.1"
  }
  ,
  "pkg": {
    "assets": [
      "bloomcrawler-dashboard.html",
      "swarm-control.html",
      "index.html",
      "data-intelligence-dashboard.html",
      "live_system_dashboard.html",
      "ai-forensics-config.html",
      "predictive-analytics-config.html",
      "threat-detection-config.html",
      "web-crawler-config.html",
      "websocket-test.html",
      "bloomcrawler.db",
      "dist/**/*",
      "public/**/*",
      "*.html",
      "*.db"
    ],
    "scripts": [
      "standalone-server.cjs",
      "standalone-server-complete.cjs",
      "complete-standalone-server.cjs",
      "core-server-module.cjs",
      "crawler-module.cjs",
      "api-routes-module.cjs",
      "advanced-features-module.cjs",
      "html-patcher-module.cjs"
    ],
    "targets": [
      "node18-win-x64",
      "node18-win-x86",
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "outputPath": "dist"
  }
}
